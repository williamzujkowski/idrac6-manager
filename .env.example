# iDRAC6 Manager Configuration
# Copy to .env and fill in your values

# Required: iDRAC host IP address
IDRAC_HOST=192.168.1.100

# iDRAC username (default: root)
IDRAC_USER=root

# Required: iDRAC password
IDRAC_PASS=changeme

# Optional: API key for protecting the REST API
# If set, all API requests must include X-API-Key header or Bearer token
IDRAC_API_KEY=
